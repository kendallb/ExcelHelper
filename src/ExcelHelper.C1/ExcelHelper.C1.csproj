    <RepositoryType>GitHub</RepositoryType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>Excel ExcelHelper Reading Writing OpenXML BIFF8 BIFF5 C1.C1Excel</PackageTags>
    <PackageReleaseNotes>Initial release using the commercial Mescius component.</PackageReleaseNotes>
  </PropertyGroup>
